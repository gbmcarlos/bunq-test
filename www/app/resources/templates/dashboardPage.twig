{% extends "layout.twig" %}

{% block title %}Dashboard{% endblock %}
{% block head %}
    {{ parent() }}
    <style type="text/css">
        .important { color: #336699; }
    </style>
{% endblock %}
{% block content %}
    {% for chat in chats %}

        chat id: {{ chat.chatId }}
        user 2 username: {{ chat.user2Username }}
        sentAt: {{ chat.createdAt | date("m/d/Y") }}

    {% endfor %}
{% endblock %}